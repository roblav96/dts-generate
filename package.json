{
	"name": "dts-generate",
	"version": "1.6.0",
	"description": "Generate TypeScript definitions for anything",
	"main": "dist/index.js",
	"files": [
		"dist"
	],
	"bin": {
		"dts-generate": "dist/cli.js"
	},
	"scripts": {
		"prepublishOnly": "del dist; tsc --declaration",
		"start": "node --require ts-node/register/transpile-only --require node-env-dev src/cli.ts",
		"watch": "cross-env NODE_ENV=development nodemon --quiet --watch src --ext ts --exec \"node --inspect --require ts-node/register/transpile-only --require node-env-dev\" src/cli.ts"
	},
	"dependencies": {
		"dts-gen": "^0.6.0",
		"fs-extra": "^9.0.1",
		"lodash": "^4.17.20",
		"mri": "^1.1.6",
		"string-fn": "^3.1.0"
	},
	"devDependencies": {
		"@types/fs-extra": "^9.0.6",
		"@types/lodash": "^4.14.167",
		"@types/mri": "^1.1.0",
		"@types/node": "^14.14.20",
		"cross-env": "^7.0.3",
		"del-cli": "^3.0.1",
		"node-env-dev": "^1.9.1",
		"nodemon": "^2.0.7",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.3"
	},
	"keywords": [
		"declarations",
		"definitions",
		"dts",
		"generate",
		"generation",
		"generator",
		"ts",
		"type",
		"types",
		"typescript"
	],
	"license": "Unlicense",
	"author": "Robert Laverty <roblav96@gmail.com> (https://github.com/roblav96)",
	"homepage": "https://github.com/roblav96/dts-generate#readme",
	"repository": "github:roblav96/dts-generate",
	"bugs": "https://github.com/roblav96/dts-generate/issues"
}
